<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanqiao.dao.UserDao">

    <select id="update"  parameterType="com.lanqiao.entity.User">
        update tb_users set username=#{username},password=#{password},email=#{email},address=#{adress} where id =#{id}
    </select>

</mapper>
